{
  "AGENT_CONFIGS": {
    "agent_class": "MADDPGAgent",
    "params": {
      "n_agents": 2,
      "lr": 0.0001,
      "gamma": 0.99,
      "batch_size": 256,
      "actor_config": [
        {"type": "Linear", "params": {"in_features": "obs_dim", "out_features": 128}, "activation": "ReLU"},
        {"type": "Linear", "params": {"in_features": 128, "out_features": 128}, "activation": "ReLU"},
        {"type": "Linear", "params": {"in_features": 128, "out_features": 524}, "activation": "ReLU"},
        {"type": "Linear", "params": {"in_features": 524, "out_features": 128}, "activation": "ReLU"},
        {"type": "Linear", "params": {"in_features": 128, "out_features": 64}, "activation": "ReLU"},
        {"type": "Linear", "params": {"in_features": 64, "out_features": "act_dim"}}
      ],
      "critic_config": [
        {"type": "Linear", "params": {"in_features": "obs_dim * n_agents + act_dim * n_agents", "out_features": 128}, "activation": "ReLU"},
        {"type": "Linear", "params": {"in_features": 128, "out_features": 128}, "activation": "ReLU"},
        {"type": "Linear", "params": {"in_features": 128, "out_features": 524}, "activation": "ReLU"},
        {"type": "Linear", "params": {"in_features": 524, "out_features": 128}, "activation": "ReLU"},
        {"type": "Linear", "params": {"in_features": 128, "out_features": 64}, "activation": "ReLU"},
        {"type": "Linear", "params": {"in_features": 64, "out_features": 1}}
      ]
    }
  },

  "EXPLORATION_CONFIGS": {
    "scheduler_class": "LinearEpsilonScheduler",
    "params": {
      "epsilon_start": 0.5,
      "epsilon_min": 0.1
    }
  },
  "DATA_CONFIGS": 0,
  "tickers": ["MSFT", "AAPL", "GOOGL", "AMZN", "TSLA", "META", "NVDA", "NFLX", "TSM", "AMD", "INTC"],
  "indicators": ["rsi", "macd", "sma"],
  "train_start": "2000-01-01",
  "train_end": "2020-12-31",
  "eval_start": "2020-01-01",
  "eval_end": "2024-12-31",

  "ENV_CONFIGS": 0,
  "initial_balance": 10000,
  "trade_cost_percent": 0.0,
  "trade_cost_fixed": 0.0,
  "train_episodes": 2,
  "eval_episodes": 2,
  
  "GENERAL_CONFIGS": 0,
  "enable_tensorboard": true,
  "verbosity": 0
  
}


