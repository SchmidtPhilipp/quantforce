{
"AGENT_CONFIGS": {
  "agent_class": "DQNAgent",
  "params": {
    "lr": 0.001,
    "gamma": 0.95,
    "batch_size": 256,
    "buffer_max_size": 1000000,
    "actor_config": [
      {"type": "Linear", "params": {"in_features": "obs_dim", "out_features": 128}, "activation": "ReLU"},
      {"type": "Linear", "params": {"in_features": 128, "out_features": 256}, "activation": "ReLU"},
      {"type": "Linear", "params": {"in_features": 256, "out_features": 512}, "activation": "ReLU"},
      {"type": "Linear", "params": {"in_features": 512, "out_features": 256}, "activation": "ReLU"},
      {"type": "Linear", "params": {"in_features": 256, "out_features": 128}, "activation": "ReLU"},
      {"type": "Linear", "params": {"in_features": 128, "out_features": 64}, "activation": "ReLU"},
      {"type": "Linear", "params": {"in_features": 64, "out_features": "act_dim"}}
    ]
  }
},
"EXPLORATION_CONFIGS": {
  "scheduler_class": "LinearEpsilonScheduler",
  "params": {
    "epsilon_start": 1.0,
    "epsilon_min": 0.01,
    "total_steps": 200
  }
},
"DATA_CONFIGS": 0,
"tickers": ["AAPL", "MSFT", "NVDA", "AMZN", "GOOGL", "GOOG", "META", "AVGO", "TSLA", "AMD"],
"indicators": ["rsi", "macd", "sma"],
"train_start": "2000-01-01",
"train_end": "2020-12-31",
"eval_start": "2000-01-01",
"eval_end": "2020-12-31",
"val_start": "2020-01-01",
"val_end": "2022-12-31",

"ENV_CONFIGS": 0,
"initial_balance": 1000000,
"trade_cost_percent": 0.01,
"trade_cost_fixed": 0.0,
"time_window_size": 1,
"rebalacing_period": 63,

"max_train_episodes": 1000,
"early_stopping_patience": 100,
"eval_interval": 1,
"val_episodes": 3,

"GENERAL_CONFIGS": 0,
"enable_tensorboard": true,
"verbosity": 0, 
"device": "cpu"
}
